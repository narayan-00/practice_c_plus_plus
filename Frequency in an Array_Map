 #include<iostream>
 #include<bits/stdc++.h>
 using namespace std;
 
 
void frequencyArray(int arr[],int n){
    map<int,int>mpp;
    for(int i=0;i<n;i++){
        mpp[arr[i]]++;
    }
    for(auto it:mpp){
        cout<<it.first<<" "<<it.second<<endl;
    }
    
}
int main(){
    int a[]={10,10,10,25,30,30};
    int n=sizeof(a)/sizeof(a[0]);
    
    frequencyArray(a,n);
    
    return 0;
     
 }
