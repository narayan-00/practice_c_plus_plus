 #include<iostream>
 using namespace std;
 
 
 bool isSorted(int a[],int n){
    for(int i=0;i<n-1;i++){
        if(a[i]>a[i+1]){
            return false;
        }
       
    }
      return true;
 }
 int main(){
    int a[]={800,100,1100,12};
    int n=sizeof(a)/sizeof(a[0]);
    cout<<(isSorted(a,n)?"True":"False") ;
     
     return 0;
     
 }
