#include<iostream>
using namespace std;
void getLargest(int a[],int n){
    int largest_index=0;
    for(int i=1;i<n;i++){
        if(a[i]>a[largest_index]){
            largest_index=i;
        }
    }
    cout<<"Largest element in array is "<<a[largest_index];
}
int main(){
    int n;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++){
        cin>>a[i];
    }
    for(int i=0;i<n;i++){
        cout<<a[i]<<" ";
    }
    cout<<endl;
    
    getLargest(a,n);
    
    return 0;
}
